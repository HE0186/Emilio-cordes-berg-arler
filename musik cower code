void setup(){
  size(500,700);
  background(255,206,180);
}
 void draw(){}
void keyPressed(){
 
  fill(21,119,40);
  ellipse(random(0,500),random(0,700),60,60);
  
  fill(255,40,0);
  ellipse(random(0,500),random(0,700),60,60);
  
  fill(0);
  ellipse(random(0,500),random(0,700),60,60);
  
  fill(253,171,159);
  ellipse(random(0,500),random(0,700),60,60);
}
